<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="plantVH"
		title="Plant"
		items="{WorkOrderModel>/ZUSPPMEG01_PLANT_F4Set}"
		search=".onValueHelpSearch"
		confirm="handleValueHelpConfirm"
		cancel=".onValueHelpClose"
		multiSelect="true">
		<StandardListItem
			title="{WorkOrderModel>Plant}"
			description="{WorkOrderModel>Description}" />
	</SelectDialog>
</core:FragmentDefinition>

